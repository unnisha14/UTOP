<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.utop">

    <application
        android:allowBackup="true"
        android:icon="@drawable/utop"
        android:label="@string/app_name"
        android:roundIcon="@drawable/utop"
        android:supportsRtl="true"
        android:theme="@style/Theme.Utop">
        <activity android:name=".Instructions"></activity>
        <activity android:name=".ThirtySixBox" />
        <activity android:name=".TwentyFiveBox" />
        <activity android:name=".SixteenBox" />
        <activity android:name=".Winner" />
        <activity android:name=".NineBox" />
        <activity android:name=".DotGameActivity" />
        <activity android:name=".SnakeGameActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>