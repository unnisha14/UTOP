<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unnisha.utop">

    <application
        android:allowBackup="true"
        android:icon="@drawable/utop"
        android:label="@string/app_name"
        android:roundIcon="@drawable/utop"
        android:supportsRtl="true"
        android:theme="@style/Theme.Utop">
        <activity android:name="com.unnisha.utop.Instructions"></activity>
        <activity android:name="com.unnisha.utop.ThirtySixBox" />
        <activity android:name="com.unnisha.utop.TwentyFiveBox" />
        <activity android:name="com.unnisha.utop.SixteenBox" />
        <activity android:name="com.unnisha.utop.Winner" />
        <activity android:name="com.unnisha.utop.NineBox" />
        <activity android:name="com.unnisha.utop.DotGameActivity" />
        <activity android:name="com.unnisha.utop.SnakeGameActivity" />
        <activity android:name="com.unnisha.utop.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>